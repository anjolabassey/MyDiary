{
  "name": "mydiary",
  "version": "1.0.0",
  "description": "MyDiary is an online diary(journal) where users can write down their experiences, thoughts and feelings.",
  "main": "server/dist",
  "scripts": {
    "start": "nodemon server/src/index.js --exec babel-node --presets env",
    "build": "babel server/src -s -D -d server/dist --presets env",
    "lint": "eslint server/src",
    "test": "mocha"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "author": "anjolabassey@yahoo.com",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-airbnb": "^2.5.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.2"
  }
}
